#!/bin/bash

# Find the process ID of the 4-to_infinity_and_beyond script
pid=$(pgrep -f "4-to_infinity_and_beyond")

# Check if the process ID was found
if [ -n "$pid" ]; then
    # Terminate the process using pkill
    pkill -P $pid

    echo "Terminated 4-to_infinity_and_beyond process"
else
    echo "No matching process found"
fi
